{
  "rules": [
    {
      "rulePriority": 1,
      "description": "Keep last 2 images with tag prefix main",
      "selection": {
        "tagStatus": "tagged",
        "tagPrefixList": [
          "main"
        ],
        "countType": "imageCountMoreThan",
        "countNumber": 2
      },
      "action": {
        "type": "expire"
      }
    },
    {
      "rulePriority": 2,
      "description": "Keep last 2 images with tag prefix develop",
      "selection": {
        "tagStatus": "tagged",
        "tagPrefixList": [
          "develop"
        ],
        "countType": "imageCountMoreThan",
        "countNumber": 2
      },
      "action": {
        "type": "expire"
      }
    },
    {
      "rulePriority": 9,
      "description": "Keep last 2 untagged images",
      "selection": {
        "tagStatus": "untagged",
        "countType": "imageCountMoreThan",
        "countNumber": 1
      },
      "action": {
        "type": "expire"
      }
    },
    {
      "rulePriority": 10,
      "description": "Remove any other images beyond last 8 tagged (catch-all)",
      "selection": {
        "tagStatus": "any",
        "countType": "imageCountMoreThan",
        "countNumber": 3
      },
      "action": {
        "type": "expire"
      }
    }
  ]
